#include <bits/stdc++.h>
#define int long long
using namespace std;
const int N=1e6+9;
int kt[N];
void sang_dem_uoc(){
    for (int i = 1; i < N; i++) {
        for (int j = i; j < N; j += i) {
            kt[j]++;
        }
    }
}
main()
{
    sang_dem_uoc();
    int t; cin >> t;
    while(t--){
        int m, n; cin >> m >> n;
        int ucln=__gcd(m,n);
        cout<<kt[ucln]<<'\n';
    }
    return 0;
}
