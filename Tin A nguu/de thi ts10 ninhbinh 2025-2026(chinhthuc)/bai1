//Bai 1 cap so nguyen to
#include <bits/stdc++.h>
#define int long long
using namespace std;
const int N=1e6+9;
int a[N];
int nt(int x){
    if(x<2)return 0;
    for(int i=2;i*i<=x;i++){
        if(x%i==0)return 0;
    }
    return 1;
}
main()
{
    int n,d=0; cin >> n;
    for(int i=1;i<=n;i++)cin >> a[i];
    for(int i=1;i<=n;i++){
        for(int j=i+1;j<=n;j++){
            int s=a[i]+a[j];
            if(nt(s))d++;
        }
    }
    cout <<d;
    return 0;
}
