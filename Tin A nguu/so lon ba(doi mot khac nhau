#include <bits/stdc++.h>
using namespace std;
const int N=1e6+9;
int a[N];
int main() {
    int n; cin >> n;
    for (int i=1;i<=n;i++) {
        cin >> a[i];
    }
    int max1 = max(max(a[1],a[2]),a[3]);
    int max3 = min(min(a[1],a[2]),a[3]);
    int max2 = a[1]+a[2]+a[3]-max1-max3;
    for (int i=4;i<=n;i++) {
        if (a[i] > max1) {
            max3 = max2;
            max2 = max1;
            max1 = a[i];
        }
        if (a[i] > max2 && a[i] < max1){
            max3 = max2;
            max2 = a[i];
        }
        if (a[i] > max3 && a[i] < max2){
            max3 = a[i];
        }
    }
    cout << max3;
    return 0;
}
